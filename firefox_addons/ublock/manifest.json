{
  "applications": {
    "gecko": {
      "id": "uBlock0@raymondhill.net",
      "strict_min_version": "52.0"
    }
  },
  "author": "All uBlock Origin contributors",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png"
    },
    "default_popup": "popup.html",
    "default_title": "uBlock Origin"
  },
  "commands": {
    "launch-element-picker": {
      "description": "__MSG_popupTipPicker__"
    },
    "launch-element-zapper": {
      "description": "__MSG_popupTipZapper__"
    },
    "launch-logger": {
      "description": "__MSG_popupTipLog__"
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "/js/vapi.js",
        "/js/vapi-client.js",
        "/js/contentscript.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "/js/scriptlets/subscriber.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extShortDesc__",
  "icons": {
    "16": "img/ublock.svg",
    "48": "img/ublock.svg",
    "96": "img/ublock.svg"
  },
  "manifest_version": 2,
  "name": "uBlock Origin",
  "options_ui": {
    "open_in_tab": true,
    "page": "dashboard.html"
  },
  "permissions": [
    "contextMenus",
    "privacy",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "uBlock\u2080",
  "version": "1.17.4",
  "web_accessible_resources": [
    "/web_accessible_resources/*"
  ]
}
